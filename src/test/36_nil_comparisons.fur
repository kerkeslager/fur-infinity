print('nil == nil => ', nil == nil, '\n')
print('nil == true => ', nil == true, '\n')
print('nil == false => ', nil == false, '\n')
print('nil == 0 => ', nil == 0, '\n')
print('nil == 1 => ', nil == 1, '\n')
print('nil == \'\' => ', nil == '', '\n')
print('nil == \'nil\' => ', nil == 'nil', '\n')

print('nil != nil => ', nil != nil, '\n')
print('nil != true => ', nil != true, '\n')
print('nil != false => ', nil != false, '\n')
print('nil != 0 => ', nil != 0, '\n')
print('nil != 1 => ', nil != 1, '\n')
print('nil != \'\' => ', nil != '', '\n')
print('nil != \'nil\' => ', nil != 'nil', '\n')
